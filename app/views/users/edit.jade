!= stylesheet_link_tag("user")
.page-header
  h1 Edit user

- form_for(user, {action: path_to.user(user), class:'form-horizontal',method: 'PUT', id: "user_form",enctype: "multipart/form-data"}, function (form) {
  != partial('users/form.jade', {locals: {form: form, user: user}})
  .form-actions
    != form.submit('Update user', {class: 'btn btn-primary'})
    != link_to('Cancel', path_to.user(user), {class: 'btn',id:'btn-cancel'})
- });
